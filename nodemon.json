{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    ".js",
    ".css"
  ],
  "verbose": true,
  "execMap": {
    "js": "tsc @ts-files.txt && del ts-files.txt && beefy main.js"
  },
  "events": {
  },
  "watch": [
    "."
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ts"
}